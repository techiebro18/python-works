<script>
    $(document).ready(function(){
        $('#filter input').change(function(){
            $("#filter").submit();
        });

        $('.sortby').click(function(){
            $('.sortby').removeClass('active');
            $(this).addClass('active');
            let sortby = $(this).data('sortby');
            //alert(sortby);
            $('.frm_sortby').val(sortby);
            $("#filter").submit();
        });
    });
</script>

