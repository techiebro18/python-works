<script>
    function zoomImg() {
        if (typeof (driftinst) !== 'undefined')
            driftinst.trigger._unbindEvents()
        driftinst = new Drift(document.querySelector('.drift-demo-trigger .typeImage.currentImg'), {
            paneContainer: document.querySelector('.detail'),
            inlinePane: 900,
            inlineOffsetY: -85,
            zoomFactor: 2,
            containInline: false
        });
    }
</script>
